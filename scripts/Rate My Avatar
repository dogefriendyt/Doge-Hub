local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "RMA Booth Games", HidePremium = false, SaveConfig = true, IntroEnabled = false})

OrionLib:MakeNotification({
	Name = "Notification",
	Content = "Have fun!",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Tab = Window:MakeTab({
	Name = "Info",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel("You must claim a booth before using any of the following scripts.")


local Tab = Window:MakeTab({
	Name = "Games",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


Tab:AddButton({
	Name = "Ping Pong",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/dogefriendyt/Rate/main/pingpong"))();
  	end    
})


Tab:AddButton({
	Name = "Screen Share",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/dogefriendyt/Rate/main/screenshare"))();
  	end   
})


Tab:AddButton({
	Name = "MS Paint",
	Callback = function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/dogefriendyt/Rate/main/paint'),true))()
  	end    
})

