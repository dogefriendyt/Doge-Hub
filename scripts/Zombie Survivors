game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = " ";
                Text = "Made by Doge Friend";
                Duration = 1.5;})


local Library = loadstring(game:HttpGet("https://pastebin.com/raw/GdN5iRbt"))() 
local Window = Library.CreateLib("Doge Hub - Zombie Survivors", "DarkTheme") 


local Tab1 = Window:NewTab("Main")
local Tab1Section = Tab1:NewSection("Scripts")

Tab1Section:NewButton("No Firerate (Must equip gun)", "", function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Notification";
                Text = "Loaded!";
                Duration = 0.5;})
local lp = game.Players.LocalPlayer.Character

local Thing = lp:FindFirstChildOfClass("Tool")
local function SetStats(StateName,Value)
   Thing:SetAttribute(StateName, Value)
end
Thing:GetAttributeChangedSignal("Cooldown"):Connect(function() SetStats("Cooldown", 0) end)
Thing:GetAttributeChangedSignal("TimesFired"):Connect(function() SetStats("TimesFired", 0) end)

end)

Tab1Section:NewButton("Bring All (Laggy af)", "", function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Notification";
                Text = "Copied to clipboard.";
                Duration = 0.5;})
local rs = game:GetService("RunService");
local lp = game.Players.LocalPlayer.Character;
rs.Heartbeat:connect(function()
    for i, v in pairs(workspace.Npcs:GetChildren()) do
        if v:FindFirstChild("HumanoidRootPart") and lp then
            v.PrimaryPart.CFrame = lp.PrimaryPart.CFrame + Vector3.new(6, 0, 0);
        end;
    end;
end);

end)

Tab1Section:NewButton("Infinity Ammo (Must equip gun)", "", function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = " ";
                Text = " ";
                Duration = 0.5;})
local lp = game.Players.LocalPlayer.Character

local Thing = lp:FindFirstChildOfClass("Tool")
local function SetStats(StateName,Value)
   Thing:SetAttribute(StateName, Value)
end
Thing:GetAttributeChangedSignal("Ammo"):Connect(function() SetStats("Ammo",math.huge) end)
Thing:GetAttributeChangedSignal("AmmoAdded"):Connect(function() SetStats("AmmoAdded",math.huge) end)
SetStats("AmmoAdded",1)

end)
